                                                            <% if( typeof name!== 'undefined') { %>
                                                                <p class="mb-15" style="color: red;"><%= name%></p>
                                                            <% } %>
                                                            <% if( typeof same!== 'undefined') { %>
                                                                <p class="mb-15" style="color: red;"><%= same%></p>
                                                            <% } %>
