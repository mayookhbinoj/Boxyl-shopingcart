        <script>

document.addEventListener('DOMContentLoaded', function () {
            document.querySelector('form').addEventListener('submit', function (event) {
   
                const image = document.getElementById('image').files[0]; // Get the selected image file

              

                // Validate image
                if (!image) {
                    document.getElementById('imageError').textContent = 'Please select an image.';
                    event.preventDefault(); // Prevent form submission
                    return;
                }

                // Check if the image file size exceeds the limit (5MB)
                if (image.size > 5 * 1024 * 1024) {
                    document.getElementById('imageError').textContent = 'Please select an image file with a size less than 5MB.';
                    event.preventDefault(); // Prevent form submission
                    return;
                }

                // Clear any previous error messages
                document.getElementById('imageError').textContent = '';

                // You can proceed with form submission or further processing here
            });
        });

            function unlist(userId){
                console.log(userId);
                Swal.fire({
               title: 'Are you sure?',
                text: 'Category will be unlisted',
                icon: 'warning',
                showCancelButton: true,
                confirmButtonText: 'Yes, unlist  it!',
                cancelButtonText: 'Cancel'
            }).then((result) => {
    if (result.isConfirmed) {
        let request = {
            method: 'PATCH'
        };
        fetch(`/admin/unlist?id=${userId}`, request)
            .then(response => response.json())
            .then(data=>{
                if (data.success) {
                    Swal.fire('unlisted', 'Thecategory has been unlisted.', 'success');
                  
                 
                   window.location.reload(true);
                } else {
                    throw new Error('Failed unlist user.');
                }
            })
            .catch(error => {
                console.error(error);
                Swal.fire('Error!', 'Failed to unlist user.', 'error');
            });
    }
});
        }
        function list(userId) {
            console.log(userId);
Swal.fire({
               title: 'Are you sure?',
                text: 'This user will be listed!',
                icon: 'warning',
                showCancelButton: false,
                confirmButtonText: 'Yes, list it!',
                cancelButtonText: 'Cancel'
            }).then((result) => {
                if (result.isConfirmed) {
                    let request = {method : 'PATCH'}
                    // Send AJAX request to unblock user
                    fetch(`/admin/list?id=${userId}`,request)
                    .then(response => response.json()) 
                    .then(data => {
                            if (data.success) {
                                Swal.fire('list!', 'The user has been list.', 'success');
                                
                                window.location.reload(true);
                              
                            } else {
                                throw new Error('Failed to list user.');
                            }
                        })
                        .catch(error => {
                            console.error(error);
                            Swal.fire('Error!', 'Failed to list categories.', 'error');
                        });
                }
            });
        }

           
