                                                            <div class="error-msg mt-1">Please add a category images.</div>
                                                            <% if( typeof image!== 'undefined') { %>
                                                                <p class="mb-15" style="color: red;"><%= imag%></p>
                                                            <% } %>
                                                        </div>
                                                    </div>
                                                    <div class="col-xxl-12 col-lg-6">
                                                        <div class="mb-3">
                                                            <label for="descriptionInput" class="form-label">Description</label>
                                                            <textarea class="form-control" id="description" rows="3" name="description" placeholder="Description" ></textarea>
                                                            <% if( typeof description!== 'undefined') { %>
                                                                <p class="mb-15" style="color: red;"><%= description%></p>
                                                            <% } %>
