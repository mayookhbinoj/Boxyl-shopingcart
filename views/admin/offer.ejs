  <div class="main-content">
    <div class="page-content">
      <div class="container-fluid">
        <!-- start page title -->
        <div class="row">
          <div class="col-12">
            <div
              class="page-title-box d-sm-flex align-items-center justify-content-between"
            >
              <h4 class="mb-sm-0">Coupons</h4>

              <div class="page-title-right">
                <ol class="breadcrumb m-0">
                  <li class="breadcrumb-item">
                    <a href="javascript: void(0);">Toner</a>
                  </li>
                  <li class="breadcrumb-item active">Coupons</li>
                </ol>
              </div>
            </div>
          </div>
        </div>
        <!-- end page title -->
        <div id="couponsList">
          <div class="row">
            <div class="col-xxl-12">
              <div class="card">
                <div class="card-body">
                  <div class="row align-items-center">
                    <div class="col-xxl-3 col-md-5"></div>
                    <!--end col-->
                    <div class="col-md-auto ms-auto">
                      <a href="/admin/addProductoffer" data-bs-target="#addModal" data-bs-toggle="modal" class="btn btn-success add-btn">
                        <i class="bi bi-plus-circle me-1 align-middle"></i>Add Product Offer 
                    
                    </a>
                      <a href="/admin/addCategoryOffer" data-bs-target="#addModal" data-bs-toggle="modal" class="btn btn-success add-btn">
                        <i class="bi bi-plus-circle me-1 align-middle"></i>Add Category Offer 
                    
                    </a>
                     
                    
                    </div>
                    <!--end col-->
                  </div>
                  <!--end row-->
                </div>
              </div>
              <!--end card-->
            </div>
            <!--end col-->
          </div>
          <!--end row-->

          <div class="row">
            <div class="col-lg-12">
              <div class="card">
                <div class="card-body">
                  <div class="table-responsive table-card">
                    <table
                      class="table align-middle table-nowrap"
                      id="couponTable"
                    >
                      <thead class="table-light">
                        <tr>
                          <th scope="col">Name</th>
                          <th scope="col">Discount</th>

                        </tr>
                      </thead>
                      <tbody class="list">
                     <% productData.forEach(item => {%>
                      
                        <tr>
                          <td class="discount">
                            <h5 class="mb-0 fs-16"><%= item.name  %></h5>
                          </td>
                          <% if( item.discountPercentage>0){%>

                   
                          <td class="couponTitle"><%= item.
                            discountPercentage  %>% </td>
                          <% }else{%>
                            <td class="couponTitle">0%</td>
                          <%} %>
                         
                          <td class="startDate">
                          
                          </td>
                          
                        </tr>
                     
                    <% }); %>
                      </tbody>
                    </table>
                    <div class="noresult" style="display: none">
                      <div class="text-center">
                        <lord-icon
                          src="https://cdn.lordicon.com/msoeawqm.json"
                          trigger="loop"
                          colors="primary:#121331,secondary:#08a88a"
                          style="width: 75px; height: 75px"
                        ></lord-icon>
                        <h5 class="mt-2">Sorry! No Result Found</h5>
                        <p class="text-muted mb-0">
